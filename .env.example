# Environment Variables Template

# Copy this file to .env and fill in your actual values
# Never commit the actual .env file to version control

# Google Gemini AI API Key
# Get your API key from: https://ai.google.dev/
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Yahoo Finance API Key (if required)
# Some endpoints might require an API key
VITE_YAHOO_FINANCE_API_KEY=your_yahoo_finance_api_key_here

# Development/Production Environment
NODE_ENV=development

# Application Settings
VITE_APP_NAME=Stock Prophecy
VITE_APP_VERSION=1.0.0

# API Base URLs
VITE_API_BASE_URL=http://localhost:3000
VITE_GEMINI_API_URL=https://generativelanguage.googleapis.com

# Feature Flags
VITE_ENABLE_AI_PREDICTIONS=true
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_DEBUG_MODE=false

# Server Configuration
PORT=3000
HOST=localhost

# CORS Settings
CORS_ORIGIN=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Settings
CACHE_TTL=300000

# Logging
LOG_LEVEL=info